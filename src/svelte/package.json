{
  "name": "data-editor-webview",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "yarn prettier src -c && yarn eslint .",
    "lint:fix": "yarn prettier src -w && yarn eslint . --fix"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.1",
    "@tsconfig/svelte": "^4.0.1",
    "@types/vscode-webview": "^1.57.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte": "^2.30.0",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "rollup": "^3.25.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-multi-input": "1.3.3",
    "rollup-plugin-scss": "^4.0.0",
    "rollup-plugin-svelte": "^7.1.5",
    "rollup-terser": "^1.0.3",
    "sass": "^1.63.4",
    "svelte": "^3.59.1",
    "svelte-check": "^3.4.3",
    "svelte-preprocess": "^5.0.4",
    "typescript": "^5.1.3"
  },
  "eslintConfig": {
    "extends": [
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    },
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "node": true,
      "es2021": true
    }
  }
}
